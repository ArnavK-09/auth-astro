---
import { type AstroAuthConfig, getSession } from '../../server'

interface Props {
  authOpts?: AstroAuthConfig
}

const { authOpts } = Astro.props as Props

let session = await getSession(Astro.request, authOpts)
---

<div>
  <Fragment set:html={Astro.slots.render('default', [session])} />
</div>
